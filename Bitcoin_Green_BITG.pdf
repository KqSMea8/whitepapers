		blockRanges.push([lower,upper]);
	});*/

	/* INITIAL DATA GRAB */
	if(reqAPIs.length) {

		var apisToCall = [];
		$.each(reqAPIs, function(i, name) {
			apisToCall.push(apiCalls[name]());
		});

		$.when.apply(undefined, apisToCall)
		.then(function(data, textStatus, jqXHR) {

			console.log(bitg);

			/* ALL PAGES */
			if(typeof bitg.info.blocks !== 'undefined' && $.isNumeric(bitg.info.blocks)) {
				$(".live-data.current-block" ).text( addCommas(Math.round(bitg.info.blocks)) );
			}

			/* ABOUT PAGE */
			var $supplyDesc = $('#section-supply .desc-wrapper');
			if($supplyDesc.length) {
				var $supplyContent = $('<div />');

				$supplyContent.append('<p>MAX. BITCOIN GREEN SUPPLY<br><strong>21,000,000</strong></p>');
				/*if (typeof bitg.info.moneysupply !== 'undefined' && $.isNumeric(bitg.info.moneysupply)) {
					$supplyContent.append('<p>COIN SUPPLY<br><strong>'+addCommas(Math.round(bitg.info.moneysupply))+'</strong></p>');
				}*/
				if (typeof bitg.circSupply !== 'undefined' && $.isNumeric(bitg.circSupply)) {
					$supplyContent.append('<p>CURRENT SUPPLY<br><strong>'+addCommas(Math.round(bitg.circSupply))+'</strong></p>');
				}
				if (typeof bitg.mnCount !== 'undefined' && $.isNumeric(bitg.mnCount)) {
					$supplyContent.append('<p>MASTERNODES ONLINE<br><strong>'+addCommas(Math.round(bitg.mnCount))+'</strong></p>');
				}
				if (typeof bitg.circSupply !== 'undefined' && typeof bitg.mnCount !== 'undefined' && $.isNumeric(bitg.circSupply) && $.isNumeric(bitg.mnCount) ) {
					var mnCoins = bitg.mnCount * 2500;
					var mnRatio = mnCoins / bitg.circSupply;
					$supplyContent.append('<p>COINS LOCKED IN MASTERNODES<br><strong>'+Math.round(mnRatio*100)+'%</strong></p>');
				}
				$supplyDesc.html($supplyContent);
			}

			/* MASTERNODES PAGE */
			var $masternodesCount = $("#active-masternodes .desc-wrapper strong" );
			if($masternodesCount.length) {
				if (typeof bitg.mnCount !== 'undefined' && $.isNumeric(bitg.mnCount)) {
					$masternodesCount.text(bitg.mnCount);
				}
			}

		});

	}

	/*var $priceUsd = $(".live-data.price-usd" );
