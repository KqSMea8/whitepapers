<script>
(function() {
	"use strict";
	
	/*if (typeof jQuery === "undefined") {
		console.log("jQuery not loaded.");
		return;
	}*/

	function addCommas(num) {
		return num.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
	}

	var $ = jQuery.noConflict();
	var debug = false;
	var reqAPIs = [];
	var apiCalls = [];
	var logIt = function(data) { if(debug) console.log(data); };
	var urlPath = window.location.pathname;
	var bitg = {
		"info": null,
		"circSupply": null,
		"mnCount": null
	};

	/*jQuery.fn.extend({
		isUndefined: function() {
			return (this === 'undefined');
		});
	});*/

	$.ajaxSetup({
		type: 'GET',
		dataType:'json'
	});

	apiCalls = {
		'info': function() {
			return $.ajax({
				url: "https://explorer.savebitcoin.io/api/getinfo"/*,
				success: function(json) {
					bitg.info=json;
				}*/
			})
			.done(function(json) {
				bitg.info=json;
			})
			.always(logIt)
		},
		'circSupply': function() {
			return $.ajax({
				url: "https://explorer.savebitcoin.io/ext/getcirculatingsupply"
			})
			.done(function(supply) {
				bitg.circSupply = supply;
			})
			.always(logIt)
		},
		'mnCount': function() {
			return $.ajax({
				url: "https://explorer.savebitcoin.io/ext/getmasternodecount",
				dataType: 'text'
			})
			.done(function(count) {
				bitg.mnCount = count;
			})
			.always(logIt)
		}
	};

	/* Determine required API calls per page type */
	reqAPIs.push('info');
	// About page
	if (/\/about/.test(urlPath)) {
		reqAPIs.push('circSupply');
		reqAPIs.push('mnCount');
	}
	if (/\/masternodes/.test(urlPath)) {
		reqAPIs.push('mnCount');
	}
	// if ($.inArray('info')>-1) reqAPIs.push('info');

	/* BLOCK REWARD TIERS */
	/*var blockIncrements = [0,200,5000,25000,100000,1050000,2100000,3150000];
