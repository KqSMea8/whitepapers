	if(window.location.pathname == '/team/') {

		$('body').addClass('enable-bio');

		var $overlay = $('<div class="bio-overlay"><div class="row sqs-row"><div class="col sqs-col-1 span-1">&nbsp;</div><div class="col sqs-col-10 span-10"><h3 class="bio-name">&nbsp;</h3><div class="bio-content">&nbsp;</div><div class="bio-close">Click to close</div></div><div class="col sqs-col-1 span-1">&nbsp;</div></div></div>');

		$('main').append($overlay);

		function showOverlay(name, content) {
			$overlay.find('.bio-name').html( name );
			$overlay.find('.bio-content').text( content );
			$('body').css({ 'overflow-y':'hidden' });
			$overlay.css({ 'display':'block'});
		}
		function hideOverlay() {
			$overlay.css({ 'display':'none'});
			$('body').css({ 'overflow-y':'scroll' });
		}

		$overlay.on('click', function(e) {
			hideOverlay();
		});

		$.each($('.image-block'), function(i, el) {

			var $profile = $(el);

			var $name = $profile.find('.image-card-wrapper .image-title-wrapper');
			var $bio = $profile.find('.image-card-wrapper .image-subtitle-wrapper');
			var $button = $profile.find('.image-card-wrapper .image-button-wrapper');

			if($bio.length) {

				$profile.addClass('clickable');
				$button.find('a').on('click', function(e) {
					e.preventDefault();
				});
				$profile.on('click', function(e) {
					showOverlay($name.find('.image-title').html(), $bio.text());
				});
			}
			// console.log($name.text());
			// console.log($bio.text());
		});

	}

})(jQuery);
